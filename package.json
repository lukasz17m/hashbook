{
  "name": "hashbook",
  "version": "1.0.0",
  "description": "A simple webbook with markdowned and #hashtagged notes.",
  "keywords": [
    "notebook",
    "hashtag",
    "markdown"
  ],
  "author": "Åukasz Mastela",
  "license": "MIT",
  "main": "app.js",
  "engines": {
    "node": "8.7.0",
    "npm": "5.4.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "dev": "cross-env NODE_ENV=development webpack",
    "lint": "npm run lint:es && npm run lint:sass",
    "lint:es": "eslint --ext .js,.vue .",
    "lint:sass": "sass-lint -v -c .sass-lint.yml src/assets/scss/**/*.scss",
    "server:dev": "cross-env NODE_ENV=development node app.js",
    "server:prod": "cross-env NODE_ENV=production node app.js",
    "start": "npm run server:dev",
    "test": "cross-env NODE_ENV=test mocha 'test/**/*.test.js'",
    "test:vue": "cross-env NODE_ENV=test mocha-webpack --webpack-config webpack.config.js --require test/vue/setup.js 'test/vue/**/*.spec.js'",
    "watch": "cross-env NODE_ENV=development webpack --watch"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "bulma": "^0.6.1",
    "chalk": "^2.3.0",
    "compression": "^1.7.1",
    "custom-event-polyfill": "^0.3.0",
    "detect-it": "^3.0.3",
    "express": "^4.16.2",
    "extend": "^3.0.1",
    "font-awesome": "^4.7.0",
    "highlight.js": "^9.12.0",
    "markdown-it": "^8.4.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-ins": "^2.0.0",
    "markdown-it-mark": "^2.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "mongoose": "^4.13.5",
    "moxios": "^0.4.0",
    "seedrandom": "^2.4.3",
    "twemoji": "^2.5.0",
    "vue": "^2.5.9",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browserslist": "^2.9.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "copy-webpack-plugin": "^4.2.3",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.12.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-chai-friendly": "^0.4.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.8.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.0.1",
    "mocha-webpack": "^1.0.1",
    "node-glob": "^1.2.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "vue-loader": "^13.5.0",
    "vue-template-compiler": "^2.5.9",
    "vue-test-utils": "^1.0.0-beta.6",
    "webpack": "^3.8.1",
    "webpack-pwa-manifest": "^3.3.2"
  }
}
